<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.submit class: "btn btn-success"%>
  <% end %>
  <%= link_to "Back to Restaurants", restaurants_path, class: "btn btn-warning" %>
</div>
